datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["createMany"]
}

enum ModelKind {
  project
  projectMembership
  user
  account
  database
  repository
}

// Project

enum ProjectRole {
  admin
  developer
  collaborator
  viewer
}

model Project {
  kind        ModelKind @default(project)
  id          String    @id @default(cuid())
  handle      String
  displayName String
  json        Json
  ownerId String


  @@unique([ownerId, handle])
}

enum newEnum {
  one
  two
  three
  four
  five
}

